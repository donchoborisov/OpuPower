{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "anhskohbo/no-captcha": "3.7.0",
        "filament/filament": "5.1.3",
        "guzzlehttp/guzzle": "7.10.0",
        "laravel/framework": "12.49.0",
        "laravel/sanctum": "4.3.0",
        "laravel/tinker": "2.11.0",
        "league/mime-type-detection": "1.16.0",
        "livewire/livewire": "4.1.1"
    },
    "require-dev": {
        "fakerphp/faker": "1.24.1",
        "laravel/sail": "1.52.0",
        "mockery/mockery": "1.6.12",
        "nunomaduro/collision": "8.8.3",
        "phpunit/phpunit": "12.5.8",
        "spatie/laravel-ignition": "2.10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "audit": {
            "ignore": [
                "PKSA-1ds2-yqqr-64g1",
                "PKSA-jyd3-2srm-pfqd"
            ]
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
